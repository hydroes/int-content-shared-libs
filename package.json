    "gulp": "^3.9.1",
    "gulp-concat": "^2.6.1",
    "gulp-minify-css": "^1.2.4",
    "gulp-sass": "^3.1.0",
    "gulp-uglify": "^2.1.2",
    "gulp-watch": "^4.3.11",
    "precommit-hook": "^3.0.0",
    "pug-lint": "^2.4.0",
    "standard": "^9.0.1",
    "validate-commit": "^3.2.1",
    "vinyl-buffer": "^1.0.0",
    "vinyl-source-stream": "^1.1.0"
  },
  "keywords": [
    "bauer",
    "shared",
    "templates"
  ],
  "license": "MIT",
  "main": "index.js",
  "maintainers": [
    {
      "name": "bdawg",
      "email": "briangouws@hotmail.com"
    }
  ],
  "name": "bauer-shared-web-components",
  "pre-commit": [
    "validate-js-standard",
    "validate-pug",
    "validate-commitmsg"
  ],
  "readme": "README.md",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/BauerXcel/shared-templates.git"
  },
  "scripts": {
    "lint": "jshint .",
    "start": "gulp",
    "test": "standard",
    "validate-js-standard": "standard",
    "validate-pug": "pug-lint components/**/*.pug",
    "validate-commitmsg": "validate-commit-msg -p eslint",
    "validate": "npm ls"
  },
  "standard": {
    "ignore": [
      "dist/**",
      "docs/**",
      "components/shared/sass/bootstrap-sass-3.3.7"
    ]
