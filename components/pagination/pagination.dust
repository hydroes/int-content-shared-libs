{!  Example usage - {>"partials/pagination" totalPages=tags_all.metadata.totalPages currentPage=tags_all.metadata.page /} !}

<ul class="pagination">
    {@paginate page=currentPage totalPages=totalPages path="{pathNoPage}/{n}/"}
        <li><a href="{path}">{n}</a></li>
    {:current}
        <li><a class="active" href="{path}">{n}</a></li>
    {:prev}
        <li><a href="{path}">&lt;</a></li>
    {:next} 
        <li><a href="{path}">&gt;</a></li>
    {/paginate}
</ul>
